<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/tools"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    app:ignore="NamespaceTypo">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="500">
        <KeyFrameSet />
    </Transition>

    <ConstraintSet
        android:id="@+id/start"
        app:visibilityMode="ignore">

        <Constraint
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_180sdp"
            android:layout_marginTop="-180dp"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/circle"
            android:layout_width="@dimen/_80sdp"
            android:layout_height="@dimen/_80sdp"
            android:layout_marginTop="-40dp"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/txt_profile_details"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_moderator_switch" />
        <Constraint
            android:id="@+id/txt_dashboard"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_profile_details" />
        <Constraint
            android:id="@+id/txt_tracker"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_dashboard"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/txt_courses"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_wishlist"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/txt_wishlist"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_tracker"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/txt_my_bank"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_courses"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/txt_reward"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_my_bank"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/txt_delete_account"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@id/txt_reward" />
        <Constraint
            android:id="@+id/txt_logout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_delete_account" />


        <Constraint
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3" />
        <Constraint
            android:id="@+id/LMSImageView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            motion:layout_constraintBottom_toBottomOf="@+id/txt_my_bank"
            motion:layout_constraintEnd_toEndOf="@+id/txt_my_bank"
            motion:layout_constraintTop_toTopOf="@+id/txt_my_bank"
            motion:visibilityMode="ignore" />

        <Constraint
            android:id="@+id/txt_moderator_switch"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/imageView3" />
        <Constraint
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="-10dp"
            motion:layout_constraintBottom_toBottomOf="@+id/txt_moderator_switch"
            motion:layout_constraintEnd_toEndOf="@+id/txt_moderator_switch"
            motion:layout_constraintTop_toTopOf="@+id/txt_moderator_switch" />


    </ConstraintSet>

    <ConstraintSet android:id="@+id/mode">
        <Constraint
            android:id="@+id/LMSImageView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"

            motion:layout_constraintBottom_toBottomOf="@+id/txt_my_bank"
            motion:layout_constraintEnd_toEndOf="@+id/txt_my_bank"
            motion:layout_constraintTop_toTopOf="@+id/txt_my_bank"
            motion:visibilityMode="ignore" />
        <Constraint
            android:id="@+id/txt_courses"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_wishlist"
            motion:visibilityMode="ignore" />

        <Constraint
            android:id="@+id/txt_my_bank"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_courses"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>

        <Constraint
            android:id="@+id/txt_reward"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_my_bank"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>
        <Constraint
            android:id="@+id/txt_courses"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="-40dp"
            android:alpha="0"
            app:visibilityMode="ignore"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/txt_wishlist"
            motion:visibilityMode="ignore">
            <PropertySet
                app:applyMotionScene="false"
                app:visibilityMode="ignore" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet
        android:id="@+id/end"
        app:visibilityMode="ignore" />


</MotionScene>